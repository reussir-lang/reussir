// RUN: %reussir-elab --mode semi %s | %FileCheck %s
enum List<T> {
    Nil,
    Cons(T, List<T>)
}

pub fn empty1(list : List<i32>) -> bool {
    match list {
        List::Nil => true,
        _ => false
    }
}

pub fn empty2(list : List<i32>) -> bool {
    match list {
        List::Nil => true,
        List::Cons(..) => false
    }
}

