// RUN: %reussir-repl -i %s -lerror | %FileCheck %s

struct [value] Point { x : i64, y : i64 }

{ let p = Point { x : 1, y : 2 } ; p.x }
// CHECK: 1 : i64

struct RcWrapper<T> { value : T }

fn wrap<T>(value : T) -> RcWrapper<T> { RcWrapper { value: value } }

wrap(Point { x : 1, y : 2 }).value.y
// CHECK: 2 : i64
